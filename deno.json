{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "start": "deno run -A --unstable-kv --watch=static/,routes/ dev.ts",
    "build": "deno run -A --unstable-kv dev.ts build",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"],
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.6.8/",
    "$std/": "https://deno.land/std@0.224.0/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "components/": "./components/",
    "constants": "./utils/constants.ts",
    "crypto-utils": "./utils/crypto-utils.ts",
    "daisyui": "npm:daisyui@4.10.3",
    "http-utils": "./utils/http-utils.ts",
    "islands/": "./islands/",
    "kv/": "./kv/",
    "lucide-preact": "npm:lucide-preact@0.363.0",
    "misc-utils": "./utils/misc-utils.ts",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "route-utils": "./utils/route-utils.ts",
    "session-utils": "./utils/session-utils.ts",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "tailwindcss/": "npm:/tailwindcss@3.4.1/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.1/plugin.js",
    "types": "./utils/types.ts",
    "z": "https://deno.land/x/zod@v3.22.4/mod.ts"
  },
  "fmt": { "lineWidth": 140 },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "nodeModulesDir": true
}
